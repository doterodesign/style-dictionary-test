{
    "font": {
        "base": {
            "family": {         "value": "{font.family.name.value}" }
        },
        "header": {
            "default": {
                "weight": {     "value": "{font.weight.regular.value}"},
                "lineHeight": { "value": "{font.lineHeight.sm.value}" }
            },
            "alt": {
                "weight": {     "value": "{font.weight.semibold.value}"},
                "lineHeight": { "value": "{font.lineHeight.md.value}" }
            }
        },
        "body": {
            "default": {
                "weight": {     "value": "{font.weight.regular.value}" },
                "lineHeight": { "value": "{font.lineHeight.lg.value}" }
            }
        }
    }
}